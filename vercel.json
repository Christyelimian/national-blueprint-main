{
  "version": 2,
  "name": "premium-homes",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/ng/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/sl/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bw/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bf/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/programs/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/about/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/delivery/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/what-we-do",
      "destination": "/index.html"
    },
    {
      "source": "/investor-portal",
      "destination": "/index.html"
    },
    {
      "source": "/engage",
      "destination": "/index.html"
    },
    {
      "source": "/impact",
      "destination": "/index.html"
    },
    {
      "source": "/insights",
      "destination": "/index.html"
    },
    {
      "source": "/delivery-track-record",
      "destination": "/index.html"
    },
    {
      "source": "/projects/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
