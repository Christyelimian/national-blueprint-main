{
  "version": 2,
  "name": "premium-homes",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/ng/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/sl/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bw/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bf/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|svg|mp4|webm|css|js|ico|woff|woff2|ttf|eot)$)",
      "destination": "/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}